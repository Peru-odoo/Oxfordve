<?xml version="1.0" encoding="utf-8"?>

<odoo>
<data>
    
    <record model="ir.ui.view" id="view_branch_extension">
        <field name="name">res.branch.extension</field>
        <field name="model">res.branch</field> 
        <field name="inherit_id" ref="branch.branch_res_branch_view_form"/>
        <field name="arch" type="xml">

            <xpath expr="sheet/group/group/field[@name='telephone']" position="after">
                <field name="abbrv" style="text-transform: uppercase;"/>
            </xpath>
            
        </field>
    </record>         
   

</data>
</odoo>


